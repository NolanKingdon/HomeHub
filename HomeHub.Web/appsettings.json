{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SpotifySortOptions": {
    "Interval": 30000,
    "MaxPlaylistResults" : 100,
    "MaxSongResults": 100,
    "MaxConcurrentThreads" : 50
  },
  "ipAccess": {
    "whitelist": "192.168.[0-9]{1,3}\\.[0-9]{1,3};127.0.0.1"
  },
  "TemperatureGuage": {
    "Unit": 0,
    "CommandInterface": "cmd.exe",
    "CommandArgs": "qterminal -e \"cat /sys/class/thermal/thermal_zone*/temp\""
  }
}
