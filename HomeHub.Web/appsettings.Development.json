{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SpotifySortOptions": {
    "Interval": 10000,
    "MaxPlaylistResults" : 100,
    "MaxSongResults": 100,
    "MaxConcurrentThreads" : 50
  },
  "ipAccess": {
    "whitelist": "192.168.[0-9]{1,3}\\.[0-9]{1,3};127.0.0.1"
  },
  "TemperatureGuage": {
    "Unit": 0,
    "CommandInterface": "cmd.exe",
    "CommandArgs": "/C ECHO 40000"
  },
  "SystemStorage": {
    "Unit": 3,
    "CommandInterface": "cmd.exe",
    "CommandArgs": "/C ECHO \"Filesystem        1B-blocks         Used    Available Use% Mounted on\nudev             1937326080            0   1937326080   0% /dev\""
  }
}
